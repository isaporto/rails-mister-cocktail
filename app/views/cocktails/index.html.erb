<div class="container">
  <h1>Cocktails</h1>

  <ul>
    <% @cocktails.each do |cocktail| %>
      <li>
        <%= cocktail.name %>
        <%= link_to "See details", cocktail_path(cocktail.id) %>
      </li>
    <% end %>
  </ul>

  <%= link_to  "Create a new cocktail", new_cocktail_path %>
</div>